#!/bin/bash
dir=$1
v=$2
dest=list-emctts-v$v
echo Making $dest using directory $dir
cat prelist-emctts-v$v|while read x; do
  set $(echo $x)
  if [[ ${1:0:1} != "#" ]]; then
    N=$1;w=$2
    maxt=${10:-1e10}
    tlogt=${11:-0}
    sum=output/weightmode$w/$((N*N*8)).strat13/summary
    for seed in `seq $8 $9`; do
      ans=`grep -v ^# $sum|head -$seed|tail -1|awk '{print $2;}'`
      for nbl in `seq $5 $7 $6`; do
        echo $dir/emctts-m25-N$N-s$seed-w$w-P$3,-$nbl $dir/qubo -N$N -m25 -w$w -p$4 -s$seed -P$3,-$nbl,$ans,$maxt,$tlogt
      done
    done
  fi
done > list-emctts-v$v
